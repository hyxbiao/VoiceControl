<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/content_frame"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <Button
        android:id="@+id/btn_qq_video_test"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/btn_qq_video_baobao"
        android:layout_marginRight="38dp"
        android:text="@string/qq_video_test" />

    <Button
        android:id="@+id/btn_qq_video_screen_max"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/btn_qq_video_baobao"
        android:layout_below="@+id/btn_qq_video_baobao"
        android:layout_marginTop="36dp"
        android:text="@string/qq_video_screen_max" />

    <Button
        android:id="@+id/btn_qq_open"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="47dp"
        android:layout_marginTop="87dp"
        android:text="@string/qq_open" />

    <Button
        android:id="@+id/btn_qq_video_baobao"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/btn_qq_open"
        android:layout_below="@+id/btn_qq_open"
        android:layout_marginTop="36dp"
        android:text="@string/qq_video_baobao" />

    <Button
        android:id="@+id/btn_qq_video_screen_min"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_alignBaseline="@+id/btn_qq_video_screen_max"
        android:layout_alignBottom="@+id/btn_qq_video_screen_max"
        android:layout_alignLeft="@+id/btn_qq_video_test"
        android:text="@string/qq_video_screen_min" />

    <Button
        android:id="@+id/btn_qq_video_close"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/btn_qq_video_screen_min"
        android:layout_below="@+id/btn_qq_video_screen_min"
        android:layout_marginTop="27dp"
        android:text="@string/qq_video_close" />

    <Button
        android:id="@+id/btn_qq_close"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_above="@+id/btn_qq_video_baobao"
        android:layout_alignLeft="@+id/btn_qq_video_test"
        android:text="@string/system_home" />

</RelativeLayout>
